{
    ".source.ola": {
        "import contract": {
            "prefix": "im",
            "body": "import '${1:contract}';"
        },
        "contract declaration": {
            "prefix": "con",
            "body": "contract ${1:Name} {\n\t$0\n}"
        },
        "enum declaration": {
            "prefix": "enum",
            "body": "enum ${1:Name} {${2:item1}, ${3:item2} }"
        },
        "function declaration": {
            "prefix": "f",
            "body": "fn ${1:name}(${2:type} ${3:name}) {\n\t$0\n}"
        },
        "function return declaration": {
            "prefix": "fr",
            "body": "fn ${1:name}(${2:type} ${3:name}) -> (${4:type} ${5:name}) {\n\t$0\n}"
        },
        "if else statement": {
            "prefix": "ife",
            "body": "if (${1:condition}) {\n\t$2\n} else {\n\t$0\n}"
        },
        "for statement": {
            "prefix": "for",
            "body": "for (var ${1:index} = 0; $1 < ${2:array}.length; $1${3:++}) {\n\t$0\n}"
        }
    }
}